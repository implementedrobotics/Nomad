struct com_state_t
{
    int64_t timestamp;
    int64_t sequence_num;

    double quat[4];        // Body Orientation Quaternion
    double theta[3];    // Body Orientation Roll Pitch Yaw
    double pos[3];      // Body Position XYZ
    double omega[3];    // Body Rotational Velocity 
    double vel[3];      // Body Velocity XYZ
}
